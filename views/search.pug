extends layout.pug

block content
  .search-bar-container
    .search-label.text-style2 Enter search term
    input.search-box(name='searchBox' placeholder='search')
  .articles-container
    .heading-text Found stories
    .article-container
      .article-title Title
      .article-subheader Subheader
      .article-author Author in Category
      .article-date Jan 1, 2020
      .article-time 5 min read
      .article-likes Likes
  .users-container
    .heading-text Found users
    .user-container
      .username Username
      .user-first-name First name
      .user-last-name Last name
  script(type="module" src="/js/search.js")
